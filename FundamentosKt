// if..else composto
// 
// com o if..else composta, somos permitidas, comparar mais de duas condições mais de duas condições (o simples e o else),
// através do else 'if'

var coracoes = 5 
    
if (coracoes >= 5){
    println("Você está, muito saudavel parábens")
}else if(coracoes == 4){
    println("Suas vidas estão sendo sucumbidas")
}else if(coracoes == 3){
    println("Você terá problemas se não cuidar de suas vidas")    
}else if(coracoes == 1){
    println("Você está morrendo")
}else{
    println("Você morreu")
} 
*/
    var dividas = 10000
    if (dividas <= -10000){
        println("Você está muito individado") 
    }else if(dividas >= +10000 ){
        println("Você está bem nas dividas")
    }else{ 
        print("Você está falido")
    }
    
    var dinheiro = 0
    if (dinheiro <= 0 ){
        println("Você está duro")    
    }else if(dinheiro >= 0){
        println("Cheio do money Cria")
    }
    
     
}--------------------------------------------------------------------------------------

