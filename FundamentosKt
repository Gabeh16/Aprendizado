
fun main() {
    println("Hello")//Exibir mensagens no console
    /* Pode ser usado em mais de uma linha 
     * 
     * 
     */
     
    //Variaveis - Elas armazenam valores 2 classes de variaveis
    
    //Mutaveis - Seu valor pode ser alterado VAR
    //Não mutaveis - Seu valor não pode ser alterado VAL
    //São maneiras diferentes de declarar uma variavel
    //Vatiaveis - String 
    //booleans - True e false 
    //String - caracteres no geral, tudo entre aspas
    //char - apenas 1 digito dentro de aspas simples 
    //INT - números inteiros Ex:100
    //double - Números decimais, com um limite de até 7 digitos após a virgula ex:22.5
    
    //float - números decimais, com um limite de até 14 digitos após a virgula arredonda o número ex:22.5f
    
    //long - números Longos com muitos digitos inteiros, ou decimial Ex:800000000000000000000000000
   
    //Estrutura de uma variável:
    
    //var none = valor
    //      ou
    //var nome: tipo
    var professor = "rodinei
    var professor: string
     
    
}
//-------------------------------------------
fun main() {
    //concatenação
    //
    /*
    var cachorro = "Rex"
    
    println("O nome do cachorro de Roberto é $cachorro")
    
   var pelé = 1
   var maradona = 2
   
    println("A posição de pelé e maradona é ${(pelé + maradona)}")
    
    var Nome = "Gabriel"
    var Idade = 16
    var cidade = "São paulo"
    
    println(" Olá meu nome é $Nome eu tenho $Idade anos de idade e moro na cidade de $cidade")
  	*/
    //Exercicio - Faça um dialogo de no minimo 15 linhas deve conter os nomes de quem estão falando de forma concatenada 
    //usando variaveis.
    var Herói = "Batman"
    var Vilão = "Coringa"
    var Jovem = "Jeannie"
    
    println("Em uma noite fria, o herói da escuridão $Herói estava a espreita observando o caos eminente causado")
    println("por seu inimigo eterno $Vilão, que estava a assaltar o banco de Gotham, após $Vilão ter rendido a todos")
    println("$Herói desse do telhado, ao som de uma bomba de fumaça jogada por ele mesmo, com a intenção de se esconder ")
    println("$Herói se esconde entre os reféns, e assim percebe que $Vilão estava rendendo uma linda garota,")
    println("que trabalhava como assistente no banco de Gotham ajudando e auxiliando os idosos, seu nome é $Jovem")
    println("$Herói percebendo a covardia de coringa primeiro decide libertar os reféns e causar uma rebelião")
    println("e assim ele faz, todos começaram a correr e isso acabou por gerar um caos sem precedentes obrigando assim $Vilão")
    println("a tomar alguma decisão, e então ele acaba por tomar uma decisão, ele diz a $Herói que não sairia sem matar $Jovem, ")
	println("mas $Herói ja havia se antecipado e sabia o que coringa poderia fazer, e então a tempos atrás ele havia")
    println("se antecipando colocando um drone que iria atrás de $Vilão, quando $Herói o acionar ele irá atrás de $Vilão")
    println("e assim $Herói faz o drone se move atrás de $Vilão e ele ameaça atirar em senhorita $Jovem, mas o drone")
    println("já estava se aproximando dele, e então ele decide largar senhorita $Jovem e fugir do banco de Gotham com o dinheiro,")
    println("mas $Herói já sabendo o que $Vilão faria pegou o dinheiro e segurou senhorita $Jovem deixando assim $Vilão")
    println("sem nada, e as pessoas com suas vidas intactas.")
}
fun main() {
    
    var halls = 7
    var freegells = 6
    
    println(halls / freegells)
  
    /*
    //Exercicio - faça uma calculadora que contenha um dos operadores matematicos como resultado
    //do cálculo de duas variaveis (contendo resultado para +, -, /, *, e %)
    */
    
    var Claudia = 3
    var Glaucia = 6
    var Valdirene = 4
    var Wasgner = 5
    var Cledson = 7
    var Rolailson = 3
    
    println("Calculos foram feitos para saber quantos números de loteria cada um dos três homens e mulheres tinham,")
    println("Claudia mais glaucia menos Valdirene divido por Wasgner foi igual à ${(Claudia + Glaucia - Valdirene / Wasgner)}")
    println("E Wasgner Multiplicado por Cledson dividindo Rolailnson e vendo a sobra é ${(Wasgner * Cledson % Rolailson)}")
    
    
    
    
    
    
    
}
fun main() {
    
    var halls = 7
    var freegells = 6
    
    println(halls / freegells)
  
    /*
    //Exercicio - faça uma calculadora que contenha um dos operadores matematicos como resultado
    //do cálculo de duas variaveis (contendo resultado para +, -, /, *, e %)
    
    
    var Claudia = 3
    var Glaucia = 6
    var Valdirene = 4
    var Wasgner = 5
    var Cledson = 7
    var Rolailson = 3
    
    println("Calculos foram feitos para saber")   
    println("quantos números")
    println("de loteria cada um")
    println("dos três homens e mulheres tinham,")
    println("Claudia mais glaucia menos Valdirene divido por Wasgner foi igual à ${(Claudia + Glaucia - Valdirene / Wasgner)}")
    println("E Wasgner Multiplicado por Cledson dividindo Rolailnson e analisando a sobra é ${(Wasgner * Cledson % Rolailson)}")
   
    //Operadores relacionais 
    //
    //== - Igual a 
    //! - Diferente de
    //! - Negando valor
    //> - Maior que 
    //< - Menor que 
    //>= - Maior ou igual
    //<= - Menor ou igual
    //
    //
    
    var claudete = 50
    var josias = 30
    var berenice = 90
    var bernadete = 80
    
    println(claudete)
    claudete++
    println(claudete)
    println("")
    println(josias)
    josias--
    println(josias)
    println("")
    println(berenice)
    berenice += claudete
    println(berenice)
    println("")
    println(bernadete)
    bernadete -= josias
    println(bernadete)
    
    
    //Condições if...else
    //
    //estrutura
    //if(condição){
    //ação
    //}
    
    var interruptor = false
    
    if(interruptor == true){
        println("Luz acesa")
    }else{
        println("Luz apagada")
    }
    */
      
    var fome = true
    
    if(fome != true){
        println("Muita fome")
    }else{
        println("Pouca Fome")
}
